{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-dev-origin-test-1"
		},
		"LS_01_KV_01_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-build-adf-test-01.vault.azure.net/"
		},
		"LS_02_ST_01_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "secretforstoragedev01"
		},
		"LS_03_ST_02_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "secretforst02"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-dev-origin-test-1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_01_KV_01_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_01_KV_01_properties_typeProperties_baseUrl')]"
					},
					"LS_02_ST_01_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_02_ST_01_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_03_ST_02_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_03_ST_02_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}